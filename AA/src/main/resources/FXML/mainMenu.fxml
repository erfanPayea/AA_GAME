<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.MainMenu">
    <children>
        <GridPane hgap="20.0" layoutX="175.0" layoutY="146.0" vgap="20.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button mnemonicParsing="false" onAction="#startGame" prefHeight="48.0" prefWidth="121.0" text="Start new game" />
                <Button mnemonicParsing="false" onAction="#twoPlayerGame" prefHeight="46.0" prefWidth="165.0" text="2Player game" GridPane.columnIndex="1" />
                <Button mnemonicParsing="false" onAction="#goSettings" prefHeight="90.0" prefWidth="218.0" text="Game settings" GridPane.rowIndex="1" />
                <Button mnemonicParsing="false" onAction="#exitGame" prefHeight="58.0" prefWidth="131.0" text="Exit game" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Button mnemonicParsing="false" onAction="#goProfile" prefHeight="49.0" prefWidth="124.0" text="Profile menu" GridPane.rowIndex="2" />
                <Button mnemonicParsing="false" onAction="#goScoreBoard" prefHeight="50.0" prefWidth="126.0" text="ScoreBoard" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
